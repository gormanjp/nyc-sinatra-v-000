<form action="/landmarks/<%=@landmark.id%>" method="POST">
	<input id="hidden" type="hidden" name="_method" value="patch">
	<%=@landmark.name%>
	<input type="text" name="landmark[name]" id="name" value="Landmark Name"><br>
	<%=@landmark.year_completed%>
	<input type="text" name="landmark[year_completed]" id="year_completed" value="Year Created"><br>
	<input type="submit" value="Update" id="Edit Landmark">
	



</form>